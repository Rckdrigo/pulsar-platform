{
  "name": "planet-prime",
  "private": true,
  "version": "1.0.0",
  "description": "Planet Prime - Frontend Boilerplate - A clean React/Vite template for building modern web applications in the Pulsar Interactive ecosystem.",
  "author": "Rodrigo Medina <https://rckdrigo.xyz>",
  "keywords": [
    "react",
    "vite",
    "boilerplate",
    "template",
    "frontend",
    "pulsar-interactive"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^6.3.5"
  }
}
